os();\">\n\t\t\t\t\t\t\t\t\t<option value=\"C\">Completo</option>\n\t\t\t\t\t\t\t\t\t<option value=\"D\">Detalhado</option>\n\t\t\t\t\t\t\t\t\t<option value=\"R\">Resumido</option>\n\t\t\t\t\t\t\t\t</select>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<div class=\"col-md-2\" id=\"detalhado1\">\n\t\t\t\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t\t\t\t<label class=\"form-label\">Agrupado</label>\n\t\t\t\t\t\t\t\t<select class=\"form-control form-white\" data-search=\"true\" name=\"agrupadoRel\" id=\"agrupadoRel\" onchange=\"habilitarCampos2();ocultarCampos();\">\n\t\t\t\t\t\t\t\t\t<option value=\"DCC\">Documentos / Centro Custo</option>\n\t\t\t\t\t\t\t\t\t<option value=\"CC\">Centro de Custo</option>\n\t\t\t\t\t\t\t\t\t<option value=\"DPC\">Documentos / Plano de Contas</option>\n\t\t\t\t\t\t\t\t\t<option value=\"PC\">Plano de Contas</option>\n\t\t\t\t\t\t\t\t\t<option value=\"C\">Cliente</option>\n\t\t\t\t\t\t\t\t\t<option value=\"V\">Vencimento</option>\n\t\t\t\t\t\t\t\t\t<option value=\"COM\">Competência</option>\n\t\t\t\t\t\t\t\t</select>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<!-- RESUMIDO -->\n\t\t\t\t\t\t<div class=\"col-md-2\" style=\"display: none\" id=\"resumido2\">\n\t\t\t\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t\t\t\t<label class=\"form-label\">Agrupado</label>\n\t\t\t\t\t\t\t\t<select class=\"form-control form-white\" data-search=\"true\" name=\"agrupadoRelResumidoResumido\" id=\"agrupadoRelResumidoResumido\">\n\t\t\t\t\t\t\t\t\t<option value=\"C\">Cliente</option>\n\t\t\t\t\t\t\t\t\t<option value=\"V\">Vencimento</option>\n\t\t\t\t\t\t\t\t\t<option value=\"COM\">Competência</option>\n\t\t\t\t\t\t\t\t</select>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<div class=\"col-md-2\">\n\t\t\t\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t\t\t\t<label class=\"form-label\">Situação</label>\n\t\t\t\t\t\t\t\t<select class=\"form-control form-white\" data-search=\"true\" name=\"situacaoRel\" id=\"situacaoRel\">\n\t\t\t\t\t\t